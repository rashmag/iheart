<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Anketa.ResultAnaliz">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/dontAnaliz"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:background="@drawable/btn_fill_rules"
        android:fontFamily="@font/manrope_semi_bold"
        android:paddingHorizontal="30dp"
        android:text="Нет анализов"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sendAnaliz" />

    <TextView
        android:id="@+id/titleResultAnaliz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/manrope_regular"
        android:text="Результаты анализов"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/analizPole1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/manrope_medium"
        android:text="Магний"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleResultAnaliz" />

    <EditText
        android:id="@+id/analizEdit1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/bottom_line_edittext"
        android:fontFamily="@font/manrope_regular"
        android:hint="Магний"
        android:imeOptions="actionDone"
        android:maxLength="3"
        android:minWidth="200dp"
        android:singleLine="true"
        android:textColor="@android:color/black"
        android:textColorHint="@android:color/darker_gray"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/analizPole1" />

    <TextView
        android:id="@+id/analizMeasurement1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/manrope_medium"
        android:text="ммоль/л"
        app:layout_constraintBottom_toBottomOf="@+id/analizEdit1"
        app:layout_constraintStart_toEndOf="@+id/analizEdit1"
        app:layout_constraintTop_toTopOf="@+id/analizEdit1" />

